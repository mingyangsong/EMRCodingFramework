<?xml version="1.0" encoding="UTF-8"?>

<analysisEngineDeploymentDescription
 xmlns="http://uima.apache.org/resourceSpecifier">
 
 <name>DRG Annotator</name>
 <description>Deploys the DRG Annotator Primitive AE</description>
 
 <deployment protocol="jms" provider="activemq">
   <casPool numberOfCASes="5"/> 
   <service>
     <inputQueue endpoint="DRGAnnotatorQueue" brokerURL="${defaultBrokerURL}"/>
     <topDescriptor>
                 <import location="./DRGAnnotatorDescriptor.xml"/>
     </topDescriptor>
   </service>
 </deployment>

</analysisEngineDeploymentDescription>
